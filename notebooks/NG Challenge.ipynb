    "vid = cv.VideoCapture(0)\n",
    "while True:\n",
    "    ret, frame = vid.read()\n",
    "    cv.imshow(\"frame\", frame)\n",
    "    \n",
    "    if cv.waitKey(1) & 0xFF == ord('q'):\n",
    "        break\n",
    "vid.release()\n",
    "cv.destroyAllWindows()"
